{% macro render_list(list_items) %}
<ul>
    {% for item in list_items %}
    <li> {{item}} </li>
    {% endfor %}
</ul>
{% endmacro %}


{% macro embed_rdfa(list, class, property) %}
    <ul>
        {% for prop in list %}
            <li><span resource ="#PDH{{class}}" typeof="dpv:PersonalDataHandling">
                <span property ="dpv:hasPersonalDataCategory" href="#{{class|replace(' ','')}}"></span>
                <span property ="dpv:has{{property}}" resource ="#{{prop|replace(' ','')}}" typeof="dpv:{{property}}">{{prop}}</span>
            </span></li>
        {% endfor %}
    </ul>
{% endmacro %}